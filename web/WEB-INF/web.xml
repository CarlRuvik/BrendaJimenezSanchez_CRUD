<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GuardarRegistro</servlet-name>
        <servlet-class>GuardarRegistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultarLista</servlet-name>
        <servlet-class>ConsultarLista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarAlumno</servlet-name>
        <servlet-class>EliminarAlumno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarDatosDelAlumno</servlet-name>
        <servlet-class>EditarDatosDelAlumno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarDatosDelAlumno2</servlet-name>
        <servlet-class>EditarDatosDelAlumno2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GuardarRegistro</servlet-name>
        <url-pattern>/GuardarRegistro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultarLista</servlet-name>
        <url-pattern>/ConsultarLista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarAlumno</servlet-name>
        <url-pattern>/EliminarAlumno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarDatosDelAlumno</servlet-name>
        <url-pattern>/EditarDatosDelAlumno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarDatosDelAlumno2</servlet-name>
        <url-pattern>/EditarDatosDelAlumno2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
